//波特率，晶振
#define BAUD 9600
#define FXTAL 8000000
//串口接收完毕中断触发声明
#pragma interrupt handler USART Received Ir: 12
//变量定义：接收缓冲变量，接收标志位
unsigned char RX_Buffer=0x00, RX_flag=0;

//函数声明
void USARI_Send(unsigned char);
//串口初始化函数
void USART_IO_Init(void)
{
     DDRD|=BIT(PD1);//PD1:TX为输出状态
}

//串口初始化函数
void USART_Init(void)
{
 unsigned int Temp;
 
 USART_IO_Init();
 
 UCSRA=0x00;   //串口控制器A清零
 UCSRB=0x00;   //串口控制器B清零
 UCSRC|=BIT(URSEL)|BIT(UCSZ1)|BIT(UCSZ0);
 
 Temp=(FXTAL/BAUD/16)-1;   //求出9600的波特率
 UBRRH=((Temp>>8)&0xff);  //高位赋值
 UBRRL=(Temp&0xff);      //低位赋值
 
 UCSRB|=BIT(TXEN)|BIT(RXEN)|BIT(RXCIE);
 
 




}

//延时函数
void delay_ms(unsigned int a)	    
{
    unsigned int j;
    while(--a!=0)
    {
	     for(j=0;j<120;j++);
    }
}




